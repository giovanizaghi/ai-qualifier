{
  "name": "ai-qualifier",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run type-check && npm run lint:check && npm run format:check",
    "quality:fix": "npm run format && npm run lint:fix",
    "analyze": "ANALYZE=true npm run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:performance": "playwright test e2e/performance.spec.ts",
    "test:accessibility": "playwright test e2e/accessibility.spec.ts",
    "test:load:basic": "k6 run load-tests/basic-load-test.js",
    "test:load:spike": "k6 run load-tests/api-spike-test.js",
    "test:load:stress": "k6 run load-tests/assessment-stress-test.js",
    "test:load:all": "npm run test:load:basic && npm run test:load:spike && npm run test:load:stress",
    "test:all": "npm run test && npm run test:e2e",
    "prepare": "husky",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "docker:dev": "./docker.sh dev:start",
    "docker:dev:stop": "./docker.sh dev:stop",
    "docker:dev:logs": "./docker.sh dev:logs --follow",
    "docker:prod": "./docker.sh prod:start",
    "docker:prod:stop": "./docker.sh prod:stop",
    "docker:prod:deploy": "./docker.sh prod:deploy",
    "docker:build": "./docker.sh build",
    "docker:build:prod": "./docker.sh build:prod",
    "docker:backup": "./docker.sh backup",
    "docker:status": "./docker.sh status",
    "docker:health": "./docker.sh health",
    "docker:cleanup": "./docker.sh cleanup"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@aws-sdk/client-s3": "^3.913.0",
    "@aws-sdk/s3-request-presigner": "^3.913.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/stripe": "^8.0.416",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-auth": "^5.0.0-beta.29",
    "openai": "^6.5.0",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "resend": "^6.2.0",
    "sonner": "^2.0.7",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "uploadthing": "^7.7.4",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.6",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/estree": "^1.0.8",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/puppeteer": "^5.4.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.0.4",
    "axe-core": "^4.11.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^27.0.1",
    "k6": "^0.0.0",
    "lighthouse": "^13.0.0",
    "lint-staged": "^16.2.4",
    "node-mocks-http": "^1.17.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "puppeteer": "^24.25.0",
    "shadcn-ui": "^0.9.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
